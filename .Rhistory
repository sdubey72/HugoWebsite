acc<-read.csv('Accidents0515.csv',header = TRUE,stringsAsFactors = FALSE)
df<-acc
df$Date<-dmy(df$Date)
df<- df%>%
filter(Date<="2005-12-31")
df$Day_of_Week<-factor(df$Day_of_Week,levels=c("1","2","3","4","5","6","7"))
df1<-df%>%
group_by(Date)%>%
summarise(daily_accidents=n())%>%
select(Date,daily_accidents)
df2<-df1%>%
filter(Date<="2005-12-31" & Date>="2005-12-01")
df3<-df%>%
group_by(Day_of_Week)%>%
summarise(num_acc=n())%>%
select(Day_of_Week,num_acc)
ggplot()+
geom_line(data = df1,aes(x=Date,y=daily_accidents))+
scale_x_date(date_breaks = '1 months',date_labels = "%b %y")+
xlab("Date")+
ylab("Accidents")
ggplot()+
geom_line(data = df2,aes(x=Date,y=daily_accidents))+
scale_x_date(date_breaks = '1 day',date_labels = "%d")+
xlab("Date")+
ylab("Accidents")
ggplot()+
geom_bar(data=df3,aes(x=Day_of_Week,y=num_acc),stat='identity',color='blue',fill='white')+
xlab('Weekday')+
ylab('Number of Accidents')
